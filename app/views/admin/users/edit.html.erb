<div class="whiteblock">
<%= form_for @display_user, url:admin_user_path do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.submit "Update Account Info" %>

<% end %>
</div>
